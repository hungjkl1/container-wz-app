{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "post",
  "info": {
    "name": "post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "active": {
      "type": "boolean"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "posts"
    }
  }
}
